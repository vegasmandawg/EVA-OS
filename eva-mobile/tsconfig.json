{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "types/react-native-config.d.ts",
    "types/nativewind-env.d.ts",
    "types/declarations.d.ts",
    "nativewind-env.d.ts"
  ]
}